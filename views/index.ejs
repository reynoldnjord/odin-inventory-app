<section>
	<h1><%= title%></h1>

	<p>Welcome to <%= title %>, a sample website without css</p>

	<h1>Dynamic Content</h1>
	<% if (error==true) { %>
	<p>Error getting dynamic content</p>
	<% } else {%>
	<p>Inventory Records:</p>
	<ul>
		<li>Models count: <span><%= data.model_count %> </span></li>
		<li>Categories count: <span><%= data.category_count %> </span></li>
	</ul>
	<% } %>
</section>
